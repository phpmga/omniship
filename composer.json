{
    "name": "omniship/common",
    "type": "library",
    "description": "Common components for Omniship shipment processing library",
    "keywords": [
        "shipment",
        "transporter",
        "omniship",
        "ship",
        "carrier",
        "delivery"
    ],
    "homepage": "https://github.com/phpmga/omniship",
    "license": "MIT",
    "authors": [
        {
            "name": "Vanderson Lopes",
            "email": "vandersonlopes@yahoo.com.br"
        }
    ],
    "autoload": {
        "psr-4": { "Omniship\\Common\\" : "src/Common" },
        "classmap": ["src/Omniship.php"]
    },
    "require": {
        "php": "^5.6|^7",
        "php-http/client-common": "^1",
        "php-http/message": "^1.5",
        "php-http/discovery": "^1.2.1",
        "symfony/http-foundation": "^4.1",
        "php-http/httplug": "^1.1",
        "guzzlehttp/psr7": "^1.4"
    },
    "suggest": {
        "league/omnipay": "The default Omniship package provides a default HTTP Adapter."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
